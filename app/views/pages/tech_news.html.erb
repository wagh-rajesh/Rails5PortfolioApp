<div class="inner cover">
  <h1 class="cover-heading">Rails Tech News Feed</h1>
  <hr>
  <div class="row">
	<% @tweets.each do |tweet| %>
		<div class="col-md-6">
			<div class="tech-news">
				<div class="card">
					<%= twitter_parser tweet %>
				</div>
			</div>
		</div>
	<% end %>  	
  </div>
</div>

